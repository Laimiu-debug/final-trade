# MSW mock switch in development.
# true  -> use mock APIs
# false -> call real backend APIs
VITE_ENABLE_MSW=false

# Optional absolute base URL. Keep empty to use same-origin /api with Vite proxy.
VITE_API_BASE_URL=

# Dev proxy target for /api when using Vite dev server.
VITE_API_PROXY_TARGET=http://127.0.0.1:8000
